@article{dogan_onur_arisoy_static_2012,
	title = {{STATIC} {AND} {DYNAMIC} {ANALYSIS} {OF} {RAILWAY} {WAGON} {TRAILER} {BOGIE} {IN} {COMPUTER} {ENVIRONMENT}},
	url = {http://rgdoi.net/10.13140/2.1.1746.1125},
	doi = {10.13140/2.1.1746.1125},
	abstract = {In the present study trailer bogie, which is being used for the international rail passenger transportation, has been investigated on whole bogie model including frame, axles, wheels, primary and secondary suspension system. In this work, bogie’s static and dynamic condition results are investigated in predetermined scenarios. Considering the technical specifications of UIC-515-4, bogie’s static structure and dynamic analysis are performed and results obtained by Von-Misses stress method and modal analysis are presented. Air springs are used on secondary suspension system and helical springs and rubber cushions are used on primary suspension system. In the paper, firstly, the methodology of analysis is explained. Bogie’s frame’s parts are defined as separate bodies in a monolith structure in CAD environment which makes the meshing process easy and flexible. Dampers, which damp effects of bounce, pitch and roll and equivalent springs for the primary and secondary suspension systems are defined. Rubber cushion’s lateral rigidities and the complex geometry problems are overcome with analytical assumptions to provide equivalent linear stiffness character. Finally, the analysis made in computer environment by using finite elements method shows that the trailer bogie has strength in the safety limits and acceptable mode shapes with their natural frequencies.},
	language = {en},
	urldate = {2020-08-29},
	author = {Doğan Onur Arısoy and A Oğuzhan Ahan and Müderrisoğlu, Kenan and Bayraktar, Meral},
	year = {2012},
	note = {Publisher: Unpublished},
}


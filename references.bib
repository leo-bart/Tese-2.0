@article{bruni_vision_2022,
	title = {A vision for a lightweight railway wheelset of the future},
	issn = {0954-4097, 2041-3017},
	url = {http://journals.sagepub.com/doi/10.1177/09544097221080619},
	doi = {10.1177/09544097221080619},
	abstract = {Rail vehicle lightweighting using fibre reinforced polymer composite materials is essential for the future of rail. This is recognised as a means of reducing carbon dioxide production through lower energy consumption, as well as reducing the impact on track degradation, thus delivering improved rail capacity and performance. This paper presents an overview of the work conducted within work package three of the NEXTGEAR project focused on the ‘wheelset of the future’. Three concepts for a hybrid metallic-composite railway axle are proposed and their strengths and weaknesses are assessed. A finite element analysis on the selected concept was conducted, including a solution for the bonded joints of the metallic collars which provide the interface to the wheels and bearings. An axle mass reduction of over 63\% was shown. An overview is also provided regarding the analysis of manufacturability of the axle, non-destructive methods for axle inspection/structural health monitoring and effects of impacts from ballast stones. Finally, a preliminary evaluation of the benefits arising from the reduction of the unsprung masses is provided, based on multibody simulations of vehicle dynamics.},
	language = {en},
	urldate = {2022-05-09},
	journal = {Proceedings of the Institution of Mechanical Engineers, Part F: Journal of Rail and Rapid Transit},
	author = {Bruni, Stefano and Mistry, Preetum J and Johnson, Michael S and Bernasconi, Andrea and Carboni, Michele and Formaggioni, Davide and Carra, Guido and Macchiavello, Sergio and Ferrante, Edoardo and Kaiser, Ingo and Viñolas, Jordi and Marazzi, Irene},
